            steps{
                echo 'Building the web'
            }
        }
        stage ("Test")
        {
            steps{
                echo 'Testing the web'
            }
        }
        stage ("Deploy")
        {
            steps{
                echo 'Deploying the web'
            }
        }
        
    }
}
